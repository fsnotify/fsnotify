# Create a new symlink to a watched file.
require symlink

mkdir /dir
watch /

ln -s /dir /link
touch link/file  # Shouldn't give any events.

Output:
	create  /link

	# TODO: should also fix FEN, which seems to send a write for regular dirs as
	# well.
	fen:
		create  /link
		write   /dir

	dragonfly: # TODO: can we fix this?
		no-events
